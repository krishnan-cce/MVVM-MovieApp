<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/imageView2"
            motion:touchAnchorSide="bottom" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="409dp"
            android:layout_height="268dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="10dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginStart="10dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            android:id="@+id/scrollView2" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView2"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginStart="20dp"
            android:id="@+id/imageView6" />
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:alpha="0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_width="150dp"
            android:layout_height="180dp"
            android:layout_marginStart="5dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:id="@+id/imageView3" />
        <Constraint
            android:layout_height="268dp"
            android:layout_marginTop="10dp"
            motion:layout_constraintHorizontal_bias="0.277"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="409dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView8"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:id="@+id/scrollView2"
            android:layout_marginEnd="10dp" />
    </ConstraintSet>
</MotionScene>